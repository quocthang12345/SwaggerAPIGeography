package com.Map.utils;

import java.util.Date;


import io.jsonwebtoken.JwsHeader;
import io.jsonwebtoken.Jwts;
import io.jsonwebtoken.SignatureAlgorithm;

public class example {
	private static String generateJWT() throws Exception {

		  return Jwts.builder()
		    .setHeaderParam(JwsHeader.KEY_ID, "XL38M52TAV")
		    .setIssuer("C9QT66NNBA")
		    .setAudience("https://appleid.apple.com")
		    .setSubject("Map4DAppleID")
		    .setExpiration(new Date(System.currentTimeMillis() + (1000 * 60 * 5)))
		    .setIssuedAt(new Date(System.currentTimeMillis()))
		    .signWith(SignatureAlgorithm.ES256, "eyJhbGciOiJFUzI1NiIsInR5cCI6IkpXVCIsImtpZCI6IlhMMzhNNTJUQVYifQ.eyJpc3MiOiJDOVFUNjZOTkJBIiwiYXVkIjoiaHR0cHM6Ly9hcHBsZWlkLmFwcGxlLmNvbSIsInN1YiI6Ik1hcDREQXBwbGVJRCIsImlhdCI6MTYwMjczODI3NCwiZXhwIjoxNjExMzc4Mjc0fQ.s7_UeahYGa2qJjijS2hWZ-Z_UXWLuMdJsGTuQEILnHtEqrnHHLoORyXhDOsbgRboYADAEWXRJ7dkfnXN6Kf2Iw")
		    .compact();
		}
	
	public static void main(String[] args) {
		try {
			example e = new example();
			System.out.print(e.generateJWT());
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}

