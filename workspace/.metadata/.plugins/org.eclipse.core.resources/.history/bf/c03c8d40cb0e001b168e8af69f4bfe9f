package com.Example.model;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

import com.fasterxml.jackson.annotation.JsonAnyGetter;
import com.fasterxml.jackson.annotation.JsonProperty;

import io.swagger.annotations.ApiModelProperty;



public class Geometry implements Serializable {
	
	@ApiModelProperty(dataType = "string",value = "typePolygons")
	@JsonProperty("typePolygons")
	private String typePolygons;
	
	@ApiModelProperty(dataType = "List<String>",value = "coordinatesPoint")
	@JsonProperty("coordinatesPoint")
	private List<String> coordinatesPoint = new ArrayList<>();
	public String getTypePolygons() {
		return typePolygons;
	}
	public void setTypePolygons(String typePolygons) {
		this.typePolygons = typePolygons;
	}
	
	@JsonAnyGetter
	public List<String> getCoordinatesPoint() {
		return coordinatesPoint;
	}
	
	public void setCoordinatesPoint(List<String> coordinatesPoint) {
		this.coordinatesPoint = coordinatesPoint;
	}
	
	

}
